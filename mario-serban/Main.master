		<div id="footerright">
			<asp:HyperLink NavigateUrl="~/" runat="server">
				<img id="logofooter" src="<%= ResolveUrl("~/App/Images/LogoWhite.png")%>" alt="Mario Serban Therapy Logo White" /></asp:HyperLink>
		</div>
	</div>

</div>

</div>
